<launch>
    <arg name="namespace" default="" />
    <arg name="lighthouse" default="0xD40AC7F1e5401e03D00F5aeC1779D8e5Af4CF9f1" />
    <arg name="model" default="" />
    <arg name="payment_token" default="0x7cfd3337F9e423751C9314f9C80cbA57CA2844FE" />
    <arg name="token_to_burn" default="" />

    <group ns="$(arg namespace)">

        <node name="trader" pkg="complier_service" type="trader_node" output="screen">
            <param name="lighthouse" value="$(arg lighthouse)" />
            <param name="model" value="$(arg model)" />
            <param name="payment_token" value="$(arg payment_token)" />
            <param name="order_lifetime" value="100" />
            <param name="path_to_db" value="$(find complier_service)/db/countries.db" />
        </node>

        <node name="worker" pkg="complier_service" type="worker_node" output="screen">
            <param name="token_to_burn" value="$(arg token_to_burn)" />
        </node>

    </group>
</launch>
